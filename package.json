    "@vant/area-data": "^1.4.1",
    "@vue/runtime-core": "^3.4.33",
    "@vue/tsconfig": "^0.1.3",
    "axios": "0.27.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "fant-axios-adapter": "^0.0.6",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "inquirer": "8.0.0",
    "lint-staged": "^13.2.0",
    "mini-types": "^0.1.7",
    "miniprogram-api-typings": "^3.9.0",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.34",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "sass": "^1.59.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.5.4",
    "uni-mini-ci": "^0.0.7",
    "uni-mini-router": "^0.1.5",
    "uni-parse-pages": "^0.0.1",
    "unocss": "^0.58.9",
    "unocss-applet": "0.7.8",
    "unplugin-auto-import": "^0.15.3",
    "vite": "5.2.8",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.8.27"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "browserslist": [
    "Android >= 4.4",
    "ios >= 9"
  ],
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix --ext .js,.vue,.ts src"
  },
  "uni-app": {
    "scripts": {
      "mp-dingtalk": {
        "title": "钉钉小程序",
        "env": {
          "UNI_PLATFORM": "mp-alipay"
        },
        "define": {
          "MP-DINGTALK": true
        }
